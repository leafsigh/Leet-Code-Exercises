   "source": [
    "# Write a function that takes a string as input and returns the string reversed.\n",
    "\n",
    "# Example:\n",
    "# Given s = \"hello\", return \"olleh\".    "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "def reverseString(s):\n",
    "    l=[]\n",
    "    for i in s[-1::-1]:\n",
    "        l.append(i)\n",
    "    return ''.join(l)"
   ]
  },
  {
